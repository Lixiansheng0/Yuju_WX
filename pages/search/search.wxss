/* pages/search/search.wxss */
/* @import "../../common/weui.wxss"; */
.picker{
    
    height: 100rpx;
    line-height: 100rpx;
}
.citystart{
    height: 100rpx;
    line-height: 100rpx;
}
page{
    background-color: #f8f8f8;
}
.fl{
    float: left;
}
.fr{
    float: right;
}
.title{
    overflow: hidden;
    padding: 30rpx 0 10rpx 10rpx;
    box-sizing: border-box;
}
.leiji{
    padding: 10rpx 0 10rpx 0;
    box-sizing: border-box;
    font-size: 26rpx;
    width: 165rpx;
    height: 100rpx;
}
.line{
    width: 100%;
    height: 50rpx;
}
.line1{
    line-height: 38rpx;
}
.line2{
    line-height: 20rpx;
}
.title .num{
    width: 280rpx;
    margin-left: 6rpx;
    height: 100rpx;
    font-size: 77rpx;
    color: #8a0000;
}
.ge{
    height: 100rpx;
    margin-left: 5rpx;
}
.kuai{
    width: 2rpx;
    height: 50rpx;
}
.tiaojianxuanze{
    overflow: hidden;
    padding: 0rpx 0 10rpx 10rpx;
    box-sizing: border-box;
    background-color: #ffffff;
    border-top: 1px solid #d9d9d9;
    border-bottom: 1px solid #d9d9d9;
}
.tip{
    font-size: 26rpx;
    color: #999999;
    overflow: hidden;
    padding: 30rpx 0 10rpx 10rpx;
    box-sizing: border-box;
}
.list{
    width: 100%;
    overflow: hidden;

}
.item{
    width: 100%;
    line-height: 100rpx;
    overflow: hidden;
    border-bottom: 1px solid #d9d9d9;
}
.item3{
    border-bottom: 0;
}
.carmodel{
    width: 185rpx;
}
.cityinput{
    height: 100rpx;
    line-height: 100rpx;
}
.button_confirm{
    margin-top:30rpx ;
}